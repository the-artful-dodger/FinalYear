    "import picamera\n",
    "import os\n",
    "\n",
    "with picamera.PiCamera() as camera:\n",
    "    camera.resolution = (1024, 768)\n",
    "    camera.start_preview()\n",
    "    # Camera warm-up time\n",
    "    time.sleep(2)\n",
    "    camera.capture(os.path.join('cap_images','image1.jpg'))"
